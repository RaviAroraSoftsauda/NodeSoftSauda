{{> header}}
{{> nav}}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
              
            <h1 class="page-header">Delivery Details</h1>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="result_filter_section">
                <form method="POST" action="" id="order_filter_form">
                    <div id="form-group">
                        <label>Period</label>
                        <input type="text" class="form-control" name="delv_fr" value="{{dateformat Date.now '01/04/2018'}}" id="filterstartdate" autocomplete="off"/> 
                    </div>
                    <div id="form-group">
                        <label>To </label>
                        <input type="text" class="form-control" name="delv_to" value="{{dateformat Date.now 'DD/MM/YYYY'}}" id="filterenddate" autocomplete="off"/>
                    </div>
                     <div id="form-group">
                        <label>Seller</label>
                        {{#listbsrt party 'S' 'sl_code' 'slcode'}}
                                {{this.party_name}}
                         {{/listbsrt}}
                    </div>
                    <div id="form-group">
                        <label>Seller Broker</label>
                        {{#listbsrt party 'T' 'sb_code' 'sbcode'}}
                                {{this.party_name}}
                            {{/listbsrt}}
                    </div>
                     <div id="form-group">
                        <label>Buyer</label>
                          {{#listbsrt party 'B' 'br_code' 'brcode'}}
                                {{this.party_name}}
                            {{/listbsrt}}
                    </div>
                    <div id="form-group" style="float: right; margin-top: -86px;margin-right: -8px;">
                        <label>Buyer Broker</label>
                           {{#listbsrt party 'R' 'bb_code' 'bbcode'}}
                                {{this.party_name}}
                            {{/listbsrt}}
                    </div>
                   <div id="form-group">
                       
                        <input type="radio" class="sd_date" name="formfilter" id="sd_date" value="sd_date"/>
                         <label>Date Wise</label>
                         <input type="radio" class="form-checkbox" name="formfilter" id="sl_brok" value="sl_brok"/>
                        <label>Seller</label>
                        
                        
                        <input type="radio" class="form-checkbox" name="formfilter" id="sb_brok" value="sb_brok"/>
                        <label>Seller Broker</label>
                        
                        <input type="radio" class="form-checkbox" name="formfilter" id="br_brok" value="br_brok"/>
                        <label>Buyer</label>
                        
                        <input type="radio" class="form-checkbox"  name="formfilter" id="bb_brok" value="bb_brok"/>
                        <label>Buyer Broker</label>
                    </div>
                    <input type="button" class="btn btn-success" name="filtersubmit" id="deliveryfiltersubmit" value="submit" value="submit"/>
                </form>
            </div>
        </div>
            <div class="col-lg-12" style="margin-top: 22px;">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <i class="fa fa-bell fa-fw"></i>Delivery LIST
                </div>
                <div class="table-responsive">
