                            </tr>
                            {{/each}}
                    {{/each}} 
                </tbody>
            </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                </div>
          
        </div>
    </div>
</div>
<!-- /#page-wrapper -->
{{> footer}}